{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/web.dom-exception.stack.js\";\nimport axios from 'axios';\nimport dayjs from 'dayjs';\nexport default {\n  name: 'ListView',\n  data() {\n    return {\n      result: []\n    };\n  },\n  created() {\n    console.log('List', this.$store.state.user);\n    this.getData();\n  },\n  methods: {\n    getData() {\n      axios.post(process.env.VUE_APP_BASEURL + '/findAll').then(response => this.result = response.data.result).catch(error => console.log(error));\n    },\n    href(row) {\n      // query  > http://localhost:8800/user/findById?name=사용자&pwd=1\n      // params > http://localhost:8800/user/findById/사용자/1\n      // index.js > path: '/user/findById/:name/:pwd'\n      this.$store.commit('setUser', row);\n      sessionStorage.setItem('setUser', this.base64(row));\n      this.$router.push({\n        name: 'SelectView'\n      });\n    },\n    base64(user) {\n      return window.btoa(encodeURIComponent(JSON.stringify(user)));\n    },\n    dateTime(value) {\n      return dayjs(value).format('YYYY-MM-DD HH:mm:ss');\n    }\n  }\n};","map":{"version":3,"names":["axios","dayjs","name","data","result","created","console","log","$store","state","user","getData","methods","post","process","env","VUE_APP_BASEURL","then","response","catch","error","href","row","commit","sessionStorage","setItem","base64","$router","push","window","btoa","encodeURIComponent","JSON","stringify","dateTime","value","format"],"sources":["/Users/wangjiangling/Downloads/vue-spring/vue-app/src/views/ListView.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-3\">\n    <h1 class=\"display-1 text-center\">사용자 목록</h1>\n    <div class=\"btn-group\">\n      <router-link to=\"/user/save\" class=\"btn btn-primary\"\n        >사용자 추가</router-link\n      >\n    </div>\n    <table class=\"table table-hover mt-3\">\n      <thead class=\"table-dark\">\n        <tr>\n          <th>이름</th>\n          <th>이메일</th>\n          <th>가입날짜</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr\n          class=\"cursor-pointer\"\n          v-for=\"row in result\"\n          v-bind:key=\"row.no\"\n          v-on:click=\"href(row)\"\n        >\n          <td>{{ row.name }}</td>\n          <td>{{ row.email }}</td>\n          <td>{{ dateTime(row.regDate) }}</td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport dayjs from 'dayjs'\nexport default {\n  name: 'ListView',\n  data() {\n    return {\n      result: []\n    }\n  },\n  created() {\n    console.log('List', this.$store.state.user)\n    this.getData()\n  },\n  methods: {\n    getData() {\n      axios\n        .post(process.env.VUE_APP_BASEURL + '/findAll')\n        .then((response) => (this.result = response.data.result))\n        .catch((error) => console.log(error))\n    },\n    href(row) {\n      // query  > http://localhost:8800/user/findById?name=사용자&pwd=1\n      // params > http://localhost:8800/user/findById/사용자/1\n      // index.js > path: '/user/findById/:name/:pwd'\n      this.$store.commit('setUser', row)\n      sessionStorage.setItem('setUser', this.base64(row))\n      this.$router.push({ name: 'SelectView' })\n    },\n    base64(user) {\n      return window.btoa(encodeURIComponent(JSON.stringify(user)))\n    },\n    dateTime(value) {\n      return dayjs(value).format('YYYY-MM-DD HH:mm:ss')\n    }\n  }\n}\n</script>\n\n<style scoped>\n.cursor-pointer {\n  cursor: pointer;\n}\n</style>"],"mappings":";;AAiCA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,KAAI,MAAO,OAAM;AACxB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,OAAO,CAACC,GAAG,CAAC,MAAM,EAAE,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI;IAC1C,IAAI,CAACC,OAAO,CAAC;EACf,CAAC;EACDC,OAAO,EAAE;IACPD,OAAOA,CAAA,EAAG;MACRX,KAAI,CACDa,IAAI,CAACC,OAAO,CAACC,GAAG,CAACC,eAAc,GAAI,UAAU,EAC7CC,IAAI,CAAEC,QAAQ,IAAM,IAAI,CAACd,MAAK,GAAIc,QAAQ,CAACf,IAAI,CAACC,M