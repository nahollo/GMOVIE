{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'CreateView',\n  data() {\n    return {\n      result: {\n        name: '',\n        email: '',\n        pwd: '',\n        gender: 1\n      },\n      active: {\n        name: false,\n        email: false,\n        pwd: false\n      }\n    };\n  },\n  created() {},\n  methods: {\n    save() {\n      if (this.result.name == '') {\n        //console.log('사용자 이름을 입력해주세요.')\n        this.active.name = true;\n        return;\n      } else {\n        this.active.name = false;\n      }\n      if (this.result.email == '') {\n        //console.log('사용자 이메일를 입력해주세요.')\n        this.active.email = true;\n        return;\n      } else {\n        this.active.email = false;\n      }\n      if (this.result.pwd == '') {\n        //console.log('사용자 비밀번호를 입력해주세요.')\n        this.active.pwd = true;\n        return;\n      } else {\n        this.active.pwd = false;\n      }\n      console.log(this.result);\n      axios.put('http://localhost:8080/save', this.result) //process.env.VUE_APP_BASEURL + '\n      .then(res => {\n        console.log(res);\n        this.$store.commit('setUser', res.data.result);\n        // sessionStorage.setItem('setUser', this.base64(res.data.result))\n        this.$router.push({\n          name: 'SelectView'\n        });\n      }).catch(err => console.log(err));\n    }\n    // base64(user) {\n    //   return window.btoa(encodeURIComponent(JSON.stringify(user)))\n    // }\n  }\n};","map":{"version":3,"names":["axios","name","data","result","email","pwd","gender","active","created","methods","save","console","log","put","then","res","$store","commit","$router","push","catch","err"],"sources":["/Users/wangjiangling/Downloads/vue-spring/vue-app/src/views/CreateView.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-3\">\n    <h1 class=\"display-1 text-center\">사용자 등록</h1>\n    <form>\n      <div class=\"mb-3 mt-3\">\n        <label for=\"name\" class=\"form-label\">이름:</label>\n        <input type=\"text\" class=\"form-control\" id=\"name\" placeholder=\"이름을 입력하세요.\" name=\"name\" autocomplete=\"off\" :class=\"{ active: active.name }\" v-model=\"result.name\" />\n      </div>\n      <div class=\"mb-3 mt-3\">\n        <label for=\"email\" class=\"form-label\">이메일:</label>\n        <input type=\"email\" class=\"form-control\" id=\"email\" placeholder=\"이메일를 입력하세요.\" name=\"email\" autocomplete=\"off\" :class=\"{ active: active.email }\" v-model=\"result.email\" />\n      </div>\n      <div class=\"mb-3\">\n        <label for=\"pwd\" class=\"form-label\">비밀번호:</label>\n        <input type=\"password\" class=\"form-control\" id=\"pwd\" placeholder=\"비밀번호를 입력하세요.\" name=\"pwd\" autocomplete=\"off\" :class=\"{ active: active.pwd }\" v-model=\"result.pwd\" />\n      </div>\n      <div class=\"d-flex\">\n        <div class=\"p-2 flex-fill\">\n          <div class=\"form-check\">\n            <input type=\"radio\" class=\"form-check-input\" id=\"radio1\" name=\"gender\" v-bind:value=\"1\" checked v-model=\"result.gender\" />\n            <label class=\"form-check-label\" for=\"radio1\">남성</label>\n          </div>\n        </div>\n        <div class=\"p-2 flex-fill\">\n          <div class=\"form-check\">\n            <input type=\"radio\" class=\"form-check-input\" id=\"radio2\" name=\"gender\" v-bind:value=\"0\" v-model=\"result.gender\" />\n            <label class=\"form-check-label\" for=\"radio2\">여성</label>\n          </div>\n        </div>\n      </div>\n    </form>\n    <div class=\"d-flex\">\n      <div class=\"p-2 flex-fill d-grid\">\n        <button type=\"button\" class=\"btn btn-primary\" @click=\"save\">생성</button>\n      </div>\n      <div class=\"p-2 flex-fill d-grid\">\n        <router-link to=\"/user\" class=\"btn btn-primary\">취소</router-link>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n  name: 'CreateView',\n  data() {\n    return {\n      result: { name: '', email: '', pwd: '', gender: 1},\n      active: { name: false, email: false, pwd: false }\n    }\n  },\n  created() {},\n  methods: {\n    save() {\n      if (this.result.name == '') {\n        //console.log('사용자 이름을 입력해주세요.')\n        this.active.name = true\n        return\n      } else {\n        this.active.name = false\n      }\n      if (this.result.email == '') {\n        //console.log('사용자 이메일를 입력해주세요.')\n        this.active.email = true\n        return\n      } else {\n        this.active.email = false\n      }\n      if (this.result.pwd == '') {\n        //console.log('사용자 비밀번호를 입력해주세요.')\n        this.active.pwd = true\n        return\n      } else {\n        this.active.pwd = false\n      }\n      console.log(this.result)\n\n      axios\n        .put('http://localhost:8080/save', this.result)//process.env.VUE_APP_BASEURL + '\n        .then((res) => {\n          console.log(res)\n          this.$store.commit('setUser', res.data.result)\n          // sessionStorage.setItem('setUser', this.base64(res.data.result))\n          this.$router.push({ name: 'SelectView' })\n        })\n        .catch((err) => console.log(err))\n    }\n    // base64(user) {\n    //   return window.btoa(encodeURIComponent(JSON.stringify(user)))\n    // }\n  }\n}\n</script>\n\n<style scoped>\n.active {\n  background-color: blanchedalmond;\n}\n</style>"],"mappings":";AA2CA,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QAAEF,IAAI,EAAE,EAAE;QAAEG,KAAK,EAAE,EAAE;QAAEC,GAAG,EAAE,EAAE;QAAEC,MAAM,EAAE;MAAC,CAAC;MAClDC,MAAM,EAAE;QAAEN,IAAI,EAAE,KAAK;QAAEG,KAAK,EAAE,KAAK;QAAEC,GAAG,EAAE;MAAM;IAClD;EACF,CAAC;EACDG,OAAOA,CAAA,EAAG,CAAC,CAAC;EACZC,OAAO,EAAE;IACPC,IAAIA,CAAA,EAAG;MACL,IAAI,IAAI,CAACP,MAAM,CAACF,IAAG,IAAK,EAAE,EAAE;QAC1B;QACA,IAAI,CAACM,MAAM,CAACN,IAAG,GAAI,IAAG;QACtB;MACF,OAAO;QACL,IAAI,CAACM,MAAM,CAACN,IAAG,GAAI,KAAI;MACzB;MACA,IAAI,IAAI,CAACE,MAAM,CAACC,KAAI,IAAK,EAAE,EAAE;QAC3B;QACA