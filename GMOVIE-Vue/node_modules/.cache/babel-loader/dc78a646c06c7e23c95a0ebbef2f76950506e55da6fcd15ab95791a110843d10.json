rm-control\" id=\"pwd\" placeholder=\"비밀번호를 입력하세요.\" name=\"pwd\"  />\n      </div>\n      <div class=\"d-flex\">\n        <div class=\"p-2 flex-fill\" >\n          <div class=\"form-check\">\n            <input type=\"radio\" class=\"form-check-input\" id=\"radio1\" name=\"gender\" v-bind:value=\"true\"  />\n            <label class=\"form-check-label\" for=\"radio1\">남성</label>\n          </div>\n        </div>\n\n        <div class=\"p-2 flex-fill\" >\n          <div class=\"form-check\">\n            <input type=\"radio\" class=\"form-check-input\" id=\"radio2\" name=\"gender\" v-bind:value=\"false\"  />\n            <label class=\"form-check-label\" for=\"radio2\">여성</label>\n          </div>\n        </div>\n      </div>\n    </form>\n    <div class=\"d-flex\">\n      <div class=\"p-2 flex-fill d-grid\">\n        <a href=\"/user/editById\" class=\"btn bt-primary\">수정</a>\n      </div>\n      <div class=\"p-2 flex-fill d-grid\">\n       <a href=\"/user\" class=\"btn bt-primary\">삭제</a>\n      </div>\n      <div class=\"p-2 flex-fill d-grid\">\n        <a href=\"/user\" class=\"btn bt-primary\">취소</a>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n  name: 'SelectView',\n  data() {\n    return {\n      result: {}\n    }\n  },\n  created() {\n    this.result = this.$store.state.user\n  },\n  methods: {\n    edit() {\n      this.$router.push({ name: 'UpdateView' })\n    },\n    del() {\n      const params = { params: { no: this.result.no } }\n      axios\n        .delete(process.env.VUE_APP_BASEURL + '/delete', params)\n        .then((res) => {\n          if (res.data.state) {\n            this.cancel()\n          } else {\n            alert(res.data.message)\n          }\n        })\n        .catch((err) => console.log(err))\n    },\n    cancel() {\n      this.$store.commit('setUser', {})\n      sessionStorage.removeItem('setUser')\n      this.$router.push({ name: 'ListView' })\n    }\n  }\n}\n</script>"]